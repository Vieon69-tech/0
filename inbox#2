if (content.length < 8) {
  Bot.sendMessage("*âŒ No messages were received...*")
  return
}

var result = JSON.parse(content)
var btcc = result
for (var ii in btcc) {
  var fr = btcc[ii].from
  var id = btcc[ii].id
  var sub = btcc[ii].subject
  var dt = btcc[ii].body_text
  var to = btcc[ii].to
//  var fl = btcc[ii].attachments
var eg = parseFloat(ii) + parseInt(1)
 

var btn = [{ title: "ðŸŒ View Message In Browser ( temp-mail.io )", url: "https://temp-mail.io/en/message/"+id+"" }]
